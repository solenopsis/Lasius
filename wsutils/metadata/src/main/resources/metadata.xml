<local>
    <dir>
        <name>actionLinkGroupTemplates</name>
        <suffix>actionLinkGroupTemplate</suffix>

        <metadata>
            <type>ActionLinkGroupTemplate</type>
            <xpath>/ActionLinkGroupTemplate/name</xpath>
            <version>33.0</version>
        </metadata>
    </dir>

    <dir>
        <name>analyticSnapshots</name>
        <suffix>snapshot</suffix>

        <metadata>
            <type>AnalyticSnapshot</type>
            <wildcard>false</wildcard>
            <xpath>/AnalyticSnapshot/name</xpath>
            <version>16.0</version>
        </metadata>
    </dir>

    <dir>
        <name>classes</name>
        <suffix>cls</suffix>

        <metadata>
            <type>ApexClass</type>
            <version>16.0</version>
        </metadata>
    </dir>

    <dir>
        <name>components</name>
        <suffix>component</suffix>

        <metadata>
            <type>ApexComponent</type>
            <version>12.0</version>
        </metadata>
    </dir>

    <dir>
        <name>pages</name>
        <suffix>page</suffix>

        <metadata>
            <type>ApexComponent</type>
            <version>12.0</version>
        </metadata>
    </dir>

    <dir>
        <name>triggers</name>
        <suffix>trigger</suffix>

        <metadata>
            <type>ApexTriggers</type>
            <version>10.0</version>
        </metadata>
    </dir>

    <dir>
        <name>appMenus</name>
        <suffix>appMenu</suffix>

        <metadata>
            <type>AppMenus</type>
            <xpath>/AppMenu/appMenuItems/appMenuItem/name</xpath>
            <version>30.0</version>
        </metadata>
    </dir>

    <dir>
        <name>approvalProcesses</name>
        <suffix>approvalProcess</suffix>

        <metadata>
            <type>ApprovalProcess</type>
            <xpath>/ApprovalProcess/approvalStep/name</xpath>
            <version>28.0</version>
        </metadata>
    </dir>

    <dir>
        <name>assignmentRules</name>
        <suffix>assignmentRules</suffix>

        <metadata>
            <type>AssignmentRules</type>
            <xpath>/AssignmentRules/assignmentRule/fullName</xpath>
            <version>27.0</version>
        </metadata>
    </dir>

    <dir>
        <name>authproviders</name>
        <suffix>authprovider</suffix>

        <metadata>
            <type>AuthProvider</type>
            <xpath>/AuthProvider/providerType</xpath>
            <version>27.0</version>
        </metadata>
    </dir>

    <dir>
        <name>autoResponseRules</name>
        <suffix>autoResponseRules</suffix>

        <metadata>
            <type>AutoResponseRules</type>
            <xpath>/AutoResponseRules/autoResponseRule/fullName</xpath>
            <version>27.0</version>
        </metadata>
    </dir>

    <dir>
        <name>objects</name>
        <suffix>object</suffix>
        <wildcard>false</wildcard>
        <version>10.0</version>

        <metadata>
            <type>BusinessProcess</type>
            <xpath>/CustomObject/businessProcesses/fullName</xpath>
            <wildcard>false</wildcard>
            <version>17.0</version>
        </metadata>

        <metadata>
            <type>CompactLayout</type>
            <xpath>/CustomObject/compactLayouts/fullName</xpath>
            <version>29.0</version>
        </metadata>

        <metadata>
            <type>CustomField</type>
            <xpath>/CustomObject/fields/fullName</xpath>
            <wildcard>false</wildcard>
            <version>20.0</version>
        </metadata>

        <metadata>
            <type>FieldSet</type>
            <xpath>/CustomObject/fieldSets/fullName</xpath>
            <version>21.0</version>
        </metadata>

        <metadata>
            <type>ListView</type>
            <xpath>/CustomObject/listViews/fullName</xpath>
            <wildcard>false</wildcard>
            <version>17.0</version>
        </metadata>

        <metadata>
            <type>NamedFilter</type>
            <xpath>/CustomObject/namedFilters/fullName</xpath>
            <wildcard>false</wildcard>
            <version>17.0</version>
        </metadata>
    </dir>

    <dir>
        <name>callCenters</name>
        <suffix>callCenter</suffix>

        <metadata>
            <type>CallCenter</type>
            <version>27.0</version>
        </metadata>
    </dir>

    <dir>
        <name>communities</name>
        <suffix>community</suffix>

        <metadata>
            <type>Community</type>
            <version>27.0</version>
        </metadata>
    </dir>

    <dir>
        <name>connectedapp</name>
        <suffix>connectedapp</suffix>

        <metadata>
            <type>ConnectedApp</type>
            <version>29.0</version>
        </metadata>
    </dir>

    <dir>
        <name>corrswhitelistorigins</name>
        <suffix>corrswhitelistorigin</suffix>

        <metadata>
            <type>CorsWhiteListOrigin</type>
            <version>32.0</version>
        </metadata>
    </dir>

    <dir>
        <name>customApplicationComponents</name>
        <suffix>customApplicationComponent</suffix>

        <metadata>
            <type>CustomApplicationComponent</type>
            <version>25.0</version>
        </metadata>
    </dir>

    <dir>
        <name>applications</name>
        <suffix>app</suffix>

        <metadata>
            <type>CustomApplication</type>
            <xpath>/CustomApplication/label</xpath>
            <version>10.0</version>
        </metadata>
    </dir>

    <dir>
        <name>labels</name>
        <suffix>label</suffix>

        <metadata>
            <type>CustomApplication</type>
            <wildcard>false</wildcard>
            <xpath>/CustomLabels/label/fullName</xpath>
            <version>14.0</version>
        </metadata>
    </dir>
</local>